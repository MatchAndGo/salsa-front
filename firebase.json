{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "*",
        "headers": [
          {
            "key": "Link",
            "value": "</index.js>;rel=preload;as=script,</assets/style.css>;rel=preload;as=style"
          }
        ]
      }
    ]
  }
}